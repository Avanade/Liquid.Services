{
  "liquid": {
    "services": {
      "CreditRatingService": {
        "id": "CreditRatingService",
        "address": "https://localhost:8081",
        "resilience": {
          "circuitBreaker": {
            "enabled": true,
            "failureThreshold": 0.5,
            "samplingDuration": 10,
            "minimumThroughput": 3,
            "durationOfBreak": 10
          },
          "retry": {
            "enabled": true,
            "attempts": 3,
            "waitDuration": 200
          }
        }
      }
    }
  }
}
